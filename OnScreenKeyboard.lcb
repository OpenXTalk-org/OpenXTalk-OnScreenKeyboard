public handler OnPaint()
    variable tPath -- as Path
    variable tBounds
    variable tArray as Array

    put the color of my foreground paint into mColor
    put the color of my background paint into mBackColor
    put the color of my highlight paint into mHiliteColor
    put the color of my border paint into mStrokeColor

   if mToggleAntiAlias then
      set the antialias of this canvas to true
   else
      set the antialias of this canvas to false
   end if

   put path mBorderPath into tPath
   scale tPath by [mHorizScale,mVertScale]
   translate tPath by [mXOffset,mYOffset]
   if mOpaque then
     set the paint of this canvas to solid paint with mBackColor
     fill tPath on this canvas
   end if
   --- add props for stroke toggling
   set the stroke width of this canvas to mStrokeWidth
   set the paint of this canvas to solid paint with mStrokeColor
   stroke tPath on this canvas

   -- checkModifiers() -- this check should probably not be here

   repeat for each element tArray in mModifierBtns
      ----- Control-Left, Win-Option, Command-AltLeft, Command-AltRight, FunctionKey, MenuKey, Shift-Left, Shift-Right, Control-Right
     put path (tArray["btnpath"]) into tPath
     scale tPath by [mHorizScale,mVertScale]
     translate tPath by [mXOffset,mYOffset]

     if (tArray["highlighted"]) then
         set the paint of this canvas to solid paint with mHiliteColor
         fill tPath on this canvas
      end if

      set the paint of this canvas to solid paint with mStrokeColor
      stroke tPath on this canvas

      put path (tArray["btnlabel"]) into tPath
      scale tPath by [mHorizScale,mVertScale]
      translate tPath by [mXOffset,mYOffset]

      set the paint of this canvas to solid paint with mColor
      fill tPath on this canvas
    end repeat

    repeat for each element tArray in mKeyboardBtns
        put path (tArray["btnpath"]) into tPath
        scale tPath by [mHorizScale,mVertScale]
        translate tPath by [mXOffset,mYOffset]
        if tArray["highlighted"] then
             set the paint of this canvas to solid paint with mHiliteColor
             fill tPath on this canvas
        end if
       set the paint of this canvas to solid paint with mStrokeColor
       stroke tPath on this canvas

       put path (tArray["btnlabel"]) into tPath
       scale tPath by [mHorizScale,mVertScale]
       translate tPath by [mXOffset,mYOffset]

       set the paint of this canvas to solid paint with mColor
       fill tPath on this canvas
    end repeat

    repeat for each element tArray in mFunctionKeys
        put path (tArray["btnpath"]) into tPath
        scale tPath by [mHorizScale,mVertScale]
        translate tPath by [mXOffset,mYOffset]
        if tArray["highlighted"] then
             set the paint of this canvas to solid paint with mHiliteColor
             fill tPath on this canvas
        end if
       set the paint of this canvas to solid paint with mStrokeColor
       stroke tPath on this canvas

       put path (tArray["btnlabel"]) into tPath
       scale tPath by [mHorizScale,mVertScale]
       translate tPath by [mXOffset,mYOffset]

       set the paint of this canvas to solid paint with mColor
       fill tPath on this canvas
    end repeat

    -----------
    if mKeyboardType is not "mini-keyboard" then

         repeat for each element tArray in mArrowKeys
            put path (tArray["btnpath"]) into tPath
            scale tPath by [mHorizScale,mVertScale]
            translate tPath by [mXOffset,mYOffset]

            if tArray["highlighted"] then
                 set the paint of this canvas to solid paint with mHiliteColor
                 fill tPath on this canvas
            end if
           set the paint of this canvas to solid paint with mStrokeColor
           stroke tPath on this canvas

           put path (tArray["btnlabel"]) into tPath
           scale tPath by [mHorizScale,mVertScale]
           translate tPath by [mXOffset,mYOffset]

           set the paint of this canvas to solid paint with mColor
           fill tPath on this canvas
        end repeat

        repeat for each element tArray in mPageKeys
            put path (tArray["btnpath"]) into tPath
            scale tPath by [mHorizScale,mVertScale]
            translate tPath by [mXOffset,mYOffset]

            if tArray["highlighted"] then
                 set the paint of this canvas to solid paint with mHiliteColor
                 fill tPath on this canvas
            end if
           set the paint of this canvas to solid paint with mStrokeColor
           stroke tPath on this canvas

           put path (tArray["btnlabel"]) into tPath
           scale tPath by [mHorizScale,mVertScale]
           translate tPath by [mXOffset,mYOffset]

           set the paint of this canvas to solid paint with mColor
           fill tPath on this canvas
        end repeat
        -------
        if mKeyboardType is "full-keyboard+numpad" then

           repeat for each element tArray in mNumPadKeys
               put path (tArray["btnpath"]) into tPath
               scale tPath by [mHorizScale,mVertScale]
               translate tPath by [mXOffset,mYOffset]

               if tArray["highlighted"] then
                    set the paint of this canvas to solid paint with mHiliteColor
                    fill tPath on this canvas
               end if
              set the paint of this canvas to solid paint with mStrokeColor
              stroke tPath on this canvas

              put path (tArray["btnlabel"]) into tPath
              scale tPath by [mHorizScale,mVertScale]
              translate tPath by [mXOffset,mYOffset]

              set the paint of this canvas to solid paint with mColor
              fill tPath on this canvas
           end repeat

            if mToggleMediaKeys then
               repeat for each element tArray in mMediaKeys
                   put path (tArray["btnpath"]) into tPath
                   scale tPath by [mHorizScale,mVertScale]
                   translate tPath by [mXOffset,mYOffset]

                   if tArray["highlighted"] then
                        set the paint of this canvas to solid paint with mHiliteColor
                        fill tPath on this canvas
                   end if
                 set the paint of this canvas to solid paint with mStrokeColor
                 stroke tPath on this canvas

                 put path (tArray["btnlabel"]) into tPath
                 scale tPath by [mHorizScale,mVertScale]
                 translate tPath by [mXOffset,mYOffset]

                 set the paint of this canvas to solid paint with mColor
                 fill tPath on this canvas
               end repeat
            end if

            if mToggleLEDs then
              repeat for each element tArray in mLEDIndicators
                   put path (tArray["btnpath"]) into tPath
                   scale tPath by [mHorizScale,mVertScale]
                   translate tPath by [mXOffset,mYOffset]

                   if tArray["highlighted"] then
                        set the paint of this canvas to solid paint with mHiliteColor
                        fill tPath on this canvas
                   end if
                 set the paint of this canvas to solid paint with mStrokeColor
                 stroke tPath on this canvas

                 put path (tArray["btnlabel"]) into tPath
                 scale tPath by [mHorizScale,mVertScale]
                 translate tPath by [mXOffset,mYOffset]

                 set the paint of this canvas to solid paint with mColor
                 fill tPath on this canvas
              end repeat
            end if

      end if

  end if
end handler

