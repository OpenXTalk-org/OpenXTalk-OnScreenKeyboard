public handler OnDo(in pThingToDo as String)
   variable tArray as Array
   variable tList as List
   variable tCounter as Integer
   variable tCharCode as Integer
   variable tName as String
   checkModifiers()

   if pThingToDo is "returnKey" then --- special case for returnKey
      put "returnKey Return" into pThingToDo
   end if
   if pThingToDo is "enterKey" then --- special case for returnKey
      put "enterKey enterKey" into pThingToDo
   end if
   if pThingToDo is "tabKey" then --- special case for returnKey
      put "tabKey tab" into pThingToDo
   end if
   if pThingToDo is "backspaceKey" then --- special case for returnKey
      put "backspaceKey Backspace" into pThingToDo
   end if
   if pThingToDo is "deleteKey" then --- special case for returnKey
      put "deleteKey Delete" into pThingToDo
   end if
   if pThingToDo is "escapeKey" then --- special case for returnKey
      put "escapeKey Escape" into pThingToDo
   end if

   if pThingToDo is "cutKey" then --- special case for returnKey
      put "commandKeyDown x" into pThingToDo
   end if
   if pThingToDo is "copyKey" then --- special case for returnKey
      put "commandKeyDown c" into pThingToDo
   end if
   if pThingToDo is "pasteKey" then --- special case for returnKey
      put "commandKeyDown v" into pThingToDo
   end if
   if pThingToDo is "undoKey" then --- special case for returnKey
      put "commandKeyDown z" into pThingToDo
   end if

   split pThingToDo by " " into tList

   if element 1 of tList is "functionKey" then --- special case for functionKey
      put "F"& (element 2 of tList) into element 2 of tList
   end if
   if element 2 of tList is "\\" then ---- special case for backslash
      put "\\" into element 2 of tList
   end if
   -- log [element 2 of tList]
   -- if element 2 of tList is "\n" then --- special case for tab
      -- put "return" into element 2 of tList
   --   put "\n" into element 2 of tList
   -- end if
   --if element 2 of tList is "\t" then --- special case for tab
   --   put "\t" into element 2 of tList
      --       put "tab" into element 2 of tList
   -- end if
   if element 2 of tList is the empty string then --- special case for space
      put " " into element 2 of tList
   end if

   put element 1 of tList && element 2 of tList into mPreviousCommand
   -- log mPreviousCommand
   if element 1 of tList is "rawKeyDown" then
     repeat with tCounter from 1 up to (the number of elements in mKeyboardBtns )
         put element tCounter of mKeyboardBtns into tArray
         if tArray["charcode"] is element 2 of tList or tArray["shiftcharcode"] is element 2 of tList  then
             put true into tArray["highlighted"]
             put tArray into mKeyboardBtns[tCounter]
         end if
      end repeat
     repeat with tCounter from 1 up to 4
         put element tCounter of mArrowKeys into tArray
         if tArray["charcode"] is element 2 of tList then
             put true into tArray["highlighted"]
             put tArray into mArrowKeys[tCounter]
         end if
      end repeat
     repeat with tCounter from 1 up to (the number of elements in mFunctionKeys )
         put element tCounter of mFunctionKeys into tArray
         if tArray["charcode"] is element 2 of tList then
             put true into tArray["highlighted"]
             put tArray into mFunctionKeys[tCounter]
         end if
      end repeat
     repeat with tCounter from 1 up to (the number of elements in mPageKeys )
         put element tCounter of mPageKeys into tArray
         if tArray["charcode"] is element 2 of tList then
             put true into tArray["highlighted"]
             put tArray into mPageKeys[tCounter]
         end if
      end repeat
     repeat with tCounter from 1 up to (the number of elements in mNumPadKeys )
         put element tCounter of mNumPadKeys into tArray
         if tArray["charcode"] is element 2 of tList then
             put true into tArray["highlighted"]
             put tArray into mNumPadKeys[tCounter]
         end if
      end repeat
   end if

   if element 1 of tList is "rawKeyUp" then
       repeat with tCounter from 1 up to (the number of elements in mKeyboardBtns )
         put element tCounter of mKeyboardBtns into tArray
         if tArray["charcode"] is element 2 of tList or tArray["shiftcharcode"] is element 2 of tList then
            put false into tArray["highlighted"]
            put tArray into mKeyboardBtns[tCounter]
         end if
      end repeat
      repeat with tCounter from 1 up to 4
         put element tCounter of mArrowKeys into tArray
         if tArray["charcode"] is element 2 of tList then
             put false into tArray["highlighted"]
             put tArray into mArrowKeys[tCounter]
         end if
      end repeat
     repeat with tCounter from 1 up to (the number of elements in mFunctionKeys )
         put element tCounter of mFunctionKeys into tArray
         if tArray["charcode"] is element 2 of tList then
             put false into tArray["highlighted"]
             put tArray into mFunctionKeys[tCounter]
         end if
      end repeat
     repeat with tCounter from 1 up to (the number of elements in mPageKeys )
         put element tCounter of mPageKeys into tArray
         if tArray["charcode"] is element 2 of tList then
             put false into tArray["highlighted"]
             put tArray into mPageKeys[tCounter]
         end if
      end repeat
     repeat with tCounter from 1 up to (the number of elements in mNumPadKeys )
         put element tCounter of mNumPadKeys into tArray
         if tArray["charcode"] is element 2 of tList then
             put false into tArray["highlighted"]
             put tArray into mNumPadKeys[tCounter]
         end if
      end repeat
   end if
   ------------------------------------------------------ keyUp & keyDown only tracks the printable char keys, by key Name instead of keycode Number ------------------------------------------------------
   if element 1 of tList is "keyDown" or element 1 of tList is "KeyDown" or element 1 of tList is "keydown" then
      --put the code of (element 2 of tList) into tCharCode
      --log tCharCode
      --- repeat for each element tArray in mKeyboardBtns
      repeat with tCounter from 1 up to (the number of elements in mKeyboardBtns )
         put element tCounter of mKeyboardBtns into tArray
         if tArray["name"] is element 2 of tList or tArray["shiftname"] is element 2 of tList or tArray["optname"] is element 2 of tList then
            put true into tArray["highlighted"]
            put tArray into mKeyboardBtns[tCounter]
            exit repeat
         end if
      end repeat
       repeat with tCounter from 1 up to (the number of elements in mNumPadKeys )
         put element tCounter of mNumPadKeys into tArray
         if tArray["name"] is element 2 of tList then
            put true into tArray["highlighted"]
            put tArray into mNumPadKeys[tCounter]
            exit repeat
         end if
      end repeat
   end if
   if element 1 of tList is "keyUp" or element 1 of tList is "KeyUp" or element 1 of tList is "keyup" then
      repeat with tCounter from 1 up to (the number of elements in mKeyboardBtns )
         put element tCounter of mKeyboardBtns into tArray
         if tArray["name"] is element 2 of tList or tArray["shiftname"] is element 2 of tList or tArray["optname"] is element 2 of tList then
            put false into tArray["highlighted"]
            put tArray into mKeyboardBtns[tCounter]
            exit repeat
         end if
      end repeat
      repeat with tCounter from 1 up to (the number of elements in mNumPadKeys )
         put element tCounter of mNumPadKeys into tArray
         if tArray["name"] is element 2 of tList then
            put false into tArray["highlighted"]
            put tArray into mNumPadKeys[tCounter]
            exit repeat
         end if
      end repeat
   end if
   ----------------------------- when on arrowKey, on commandKeyDown, on optionKeyDown, on functionKey handlers are used, the keys act like momentary switch buttons ----------------------------------
   --- for these a timer is set turn off the key, this is so that the virtual keyboard actually displays the momentary key Down then Up

    if element 1 of tList is "arrowKey" then
      repeat with tCounter from 1 up to 4 -- (the number of elements in mArrowKeys )
         put element tCounter of mArrowKeys into tArray
         if tArray["name"] is element 2 of tList then
            put true into tArray["highlighted"]
            put tArray into mArrowKeys[tCounter]
            schedule timer in 0.1 seconds
            exit repeat
         end if
      end repeat
   end if
   if (element 1 of tList is "backspaceKey") or (element 1 of tList is "deleteKey") or \
      (element 1 of tList is "tabKey") or (element 1 of tList is "returnKey") or (element 1 of tList is "enterKey") then
     repeat with tCounter from 1 up to (the number of elements in mKeyboardBtns )
        put element tCounter of mKeyboardBtns into tArray
        if tArray["name"] is element 2 of tList then
           put true into tArray["highlighted"]
           put tArray into mKeyboardBtns[tCounter]
           schedule timer in 0.1 seconds
           exit repeat
        end if
     end repeat
   end if
   if (element 1 of tList is "escapeKey") then
        put element 1 of mFunctionKeys into tArray
        if tArray["name"] is element 2 of tList then
           put true into tArray["highlighted"]
           put tArray into mFunctionKeys[1]
           schedule timer in 0.1 seconds
           schedule timer in 0.2 seconds
        end if
   end if

      if element 1 of tList is "commandKeyDown" then
      --put the code of (element 2 of tList) into tCharCode
      -- log tCharCode
      repeat with tCounter from 1 up to (the number of elements in mKeyboardBtns )
         put element tCounter of mKeyboardBtns into tArray
         if tArray["name"] is element 2 of tList then
            put true into tArray["highlighted"]
            put tArray into mKeyboardBtns[tCounter]
            --- schedule timer -- do a 'commandKeyDown' here in some milliseconds
            schedule timer in 0.1 seconds
            schedule timer in 0.2 seconds
            schedule timer in 0.3 seconds
            exit repeat
         end if
      end repeat
   end if

   if element 1 of tList is "optionKeyDown" then
      --put the code of (element 2 of tList) into tCharCode
      -- log tCharCode
      repeat with tCounter from 1 up to (the number of elements in mKeyboardBtns )
         put element tCounter of mKeyboardBtns into tArray
         if tArray["optname"] is element 2 of tList or tArray["name"] is element 2 of tList then
            put true into tArray["highlighted"]
            put tArray into mKeyboardBtns[tCounter]
            --- schedule timer -- do a 'commandKeyDown' here in some milliseconds
            schedule timer in 0.1 seconds
            schedule timer in 0.2 seconds
            schedule timer in 0.3 seconds
            exit repeat
         end if
      end repeat
   end if

   if element 1 of tList is "controlKeyDown" then
      --put the code of (element 2 of tList) into tCharCode
      -- log tCharCode
      repeat with tCounter from 1 up to (the number of elements in mKeyboardBtns )
         put element tCounter of mKeyboardBtns into tArray
         if tArray["name"] is element 2 of tList then
            put true into tArray["highlighted"]
            put tArray into mKeyboardBtns[tCounter]
            --- schedule timer -- do a 'commandKeyDown' here in some milliseconds
            schedule timer in 0.1 seconds
            schedule timer in 0.2 seconds
            schedule timer in 0.3 seconds
            exit repeat
         end if
      end repeat
   end if

   if element 1 of tList is "functionKey" then
     -- when 'on functionKey' is used as a keyboard handler the F-keys act as momentary buttons, because this occurs very fast, scripts may
     -- need to set a timer to unhighlight after some milliseconds so that end users can see that the on screenkeyboard did recieve a message
    repeat with tCounter from 1 up to (the number of elements in mFunctionKeys )
         put element tCounter of mFunctionKeys into tArray
         if tArray["name"] is element 2 of tList then
            put true into tArray["highlighted"]
            put tArray into mFunctionKeys[tCounter]
            --- schedule timer -- do a 'functionKeyUp' here in some milliseconds
            schedule timer in 0.1 seconds
            schedule timer in 0.2 seconds
            exit repeat
         end if
      end repeat
    -- log tName
   end if
   redraw all
end handler

public handler checkModifiers()
   ----- Control-Left, Control-Right, Command-AltLeft, Command-AltRight, Shift-Left, Shift-Right, Win-Option, FunctionKey, MenuKey, CapsLock
   variable tArray as Array
   if the shift key is currently down then
      put element 5 of mModifierBtns into tArray
      put true into tArray["highlighted"]
      put tArray into element 5 of mModifierBtns
      put element 6 of mModifierBtns into tArray
      put true into tArray["highlighted"]
      put tArray into element 6 of mModifierBtns
      -- log "Shift"
   else
      put element 5 of mModifierBtns into tArray
      put false into tArray["highlighted"]
      put tArray into element 5 of mModifierBtns
      put element 6 of mModifierBtns into tArray
      put false into tArray["highlighted"]
      put tArray into element 6 of mModifierBtns
   end if

   if the caps lock key is currently down then
      put element 10 of mModifierBtns into tArray
      put true into tArray["highlighted"]
      put tArray into element 10 of mModifierBtns
   else
      put element 10 of mModifierBtns into tArray
      put false into tArray["highlighted"]
      put tArray into element 10 of mModifierBtns
   end if

   if the command key is currently down then
      put element 2 of mModifierBtns into tArray
      put true into tArray["highlighted"]
      put tArray into element 2 of mModifierBtns
      put element 3 of mModifierBtns into tArray
      put true into tArray["highlighted"]
      put tArray into element 3 of mModifierBtns
      --log "Command"
   else
      put element 2 of mModifierBtns into tArray
      put false into tArray["highlighted"]
      put tArray into element 2 of mModifierBtns
      put element 3 of mModifierBtns into tArray
      put false into tArray["highlighted"]
      put tArray into element 3 of mModifierBtns
   end if
   if the alt key is currently down or the option key is currently down then
      put element 7 of mModifierBtns into tArray
      put true into tArray["highlighted"]
      put tArray into element 7 of mModifierBtns
   else
      put element 7 of mModifierBtns into tArray
      put false into tArray["highlighted"]
      put tArray into element 7 of mModifierBtns
   end if
   if the control key is currently down then
      put element 1 of mModifierBtns into tArray
      put true into tArray["highlighted"]
      put tArray into element 1 of mModifierBtns
      put element 2 of mModifierBtns into tArray
      put true into tArray["highlighted"]
      put tArray into element 2 of mModifierBtns
   else
      put element 1 of mModifierBtns into tArray
      put false into tArray["highlighted"]
      put tArray into element 1 of mModifierBtns
      put element 2 of mModifierBtns into tArray
      put false into tArray["highlighted"]
      put tArray into element 2 of mModifierBtns
   end if
end handler

public handler OnTimer()
   checkModifiers()
   variable tList as List
   variable tArray as Array
   variable tCounter as Number
   if mPreviousCommand is not "" then
   split mPreviousCommand by " " into tList
   -- log [tList]
      repeat with tCounter from 1 up to (the number of elements in mKeyboardBtns )
         put element tCounter of mKeyboardBtns into tArray
         if tArray["name"] is element 2 of tList then
            put false into tArray["highlighted"]
            put tArray into mKeyboardBtns[tCounter]
            put "" into mPreviousCommand
            -- exit repeat
         end if
      end repeat
      repeat with tCounter from 1 up to (the number of elements in mArrowKeys )
         put element tCounter of mArrowKeys into tArray
         if tArray["name"] is element 2 of tList then
            put false into tArray["highlighted"]
            put tArray into mArrowKeys[tCounter]
            put "" into mPreviousCommand
            -- exit repeat
         end if
      end repeat
      repeat with tCounter from 1 up to (the number of elements in mPageKeys )
         put element tCounter of mPageKeys into tArray
         if tArray["name"] is element 2 of tList then
            put false into tArray["highlighted"]
            put tArray into mPageKeys[tCounter]
            put "" into mPreviousCommand
            -- exit repeat
         end if
      end repeat
      repeat with tCounter from 1 up to (the number of elements in mFunctionKeys )
         put element tCounter of mFunctionKeys into tArray
         if tArray["name"] is element 2 of tList then
            put false into tArray["highlighted"]
            put tArray into mFunctionKeys[tCounter]
            put "" into mPreviousCommand
            -- exit repeat
         end if
      end repeat
      repeat with tCounter from 1 up to (the number of elements in mNumPadKeys )
         put element tCounter of mNumPadKeys into tArray
         if tArray["name"] is element 2 of tList then
            put false into tArray["highlighted"]
            put tArray into mNumPadKeys[tCounter]
            put "" into mPreviousCommand
            -- exit repeat
         end if
      end repeat
   end if
   redraw all
end handler
